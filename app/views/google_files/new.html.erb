<div>
  <span><a href="<%= all_google_files_path %>">View all files</a></span>
</div>
<input type="file" id="file" name="uploads"  />

<div id="uploadFile">
  click here to upload
</div>
<script type="text/javascript">

$('#uploadFile').click(function(){
  $.ajaxUpload({
    url: "<%= google_files_path %>",
    data: ':file',
    success: function (data, status, xhr) {
      console.log(data);
    }
  });
});
</script>